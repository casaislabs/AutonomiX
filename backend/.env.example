# Backend environment example

# Server
PORT=3000
HOST=0.0.0.0

# CORS: frontend origin allowed for private /api/* routes
FRONTEND_ORIGIN=http://localhost:5173

# Chain / RPC
RPC_URL=https://sepolia.base.org

# Agent contract
AGENT_ADDRESS=0xYourAgentContractAddress

# Optional: enable write operations (use with caution in production)
ADMIN_PRIVATE_KEY=0xYourPrivateKey
ADMIN_PK=

# x402 paywall configuration (optional, for protected endpoints)
X402_NETWORK=base-sepolia
X402_RECEIVER_ADDRESS=0xYourUSDCReceiverAddress
X402_PRICE_USD=$0.001
X402_FACILITATOR_URL=https://x402.org/facilitator
X402_MAX_AGENT_IDS=100

# Limits and cache (optional)
MAX_IMAGE_BYTES=1048576
CACHE_TTL_MS=60000